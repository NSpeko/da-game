{
    "extends": "airbnb-base",
    "rules": {
        "no-undef": "warn",
        "no-param-reassign": "warn",
        "class-methods-use-this": "warn",
        "no-use-before-define": "warn",
        "default-case": "off",
        "arrow-parens": "warn",
        "function-paren-newline": "warn",
        "comma-dangle": "warn",
        "no-mixed-operators": "off",
        "no-unused-vars": "warn"
    },
    "env": {
        "browser": true,
        "node": true
    }
}